package org.string;

public class StringDemo4 {
	public static void main(String[] args) {
		String s = "sivasabari";
		
		
		for (int j = 0; j < s.length()-1; j++) {
			int count =1; 
			char ch = s.charAt(j);
		for (int i = j+1; i < s.length(); i++) {
			if (ch==s.charAt(i)) {
				count++;
			}
		}
		System.out.println(ch+" number of count is "+count);
		}
	}

}
