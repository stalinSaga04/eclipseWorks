package org.loopExcercise;

public class Pattern11 {
	public static void main(String[] args) {
		int n=0;
		for (int i = 4; i >0; i--) {
			for (int j = i; j >0 ; j--) {
				System.out.print(" ");
			}
			for (int j = 0; j >=0; j++) {
				System.out.println(n++);
			}
		}
		
		for (int i = 0; i >0; i++) {
			
		}
	}

}
