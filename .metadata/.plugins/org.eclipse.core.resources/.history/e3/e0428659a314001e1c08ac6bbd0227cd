package org.stringEx;

import java.util.ArrayList;
import java.util.Scanner;

public class Str1 {
	private boolean meth(String next) {
		// TODO Auto-generated method stub
		for (int i = 0; i < next.length(); i++) {
			if (Character.isLetterOrDigit(next.charAt(i))) 
			return false;
			
			else 
				return true;
		}
		return true;
	}
	public static void main(String[] args) {
//		boolean digit = false;
		Scanner sc = new Scanner(System.in);
		String next = "600028";//sc.nextLine();
		Str1 s  = new Str1();
		s.meth(next);
//			
		
		
		
	}

}
