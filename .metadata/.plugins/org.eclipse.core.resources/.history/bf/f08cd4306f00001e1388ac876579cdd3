package org.string;

public class StringDemo5 {
	public static void main(String[] args) {
		String s = "IAmGoingNow";
		String[] ar = new String[s.length()];
		for (int i = 0; i <s.length()-1; i++) {
			char charAt = s.charAt(i);
			if ((charAt>='A' && charAt<='Z')) {
				System.out.println();
			}
			System.out.print(s);
		}
		
	}

}
