package org.loopExcercise;

public class Pattern11 {
	public static void main(String[] args) {
		for (int i = 4; i >0; i--) {
			for (int j = i; j >i ; j++) {
				System.out.println(" ");
			}
		}
		int n=0;
		for (int i = 0; i >0; i++) {
			for (int j = 0; j <= i; j++) {
				System.out.println(n++);
			}
		}
	}

}
