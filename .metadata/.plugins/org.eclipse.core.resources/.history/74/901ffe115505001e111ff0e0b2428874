package org.loopExcercise;

public class Pattern11 {
	public static void main(String[] args) {
		int n=5;
		for (int i = 1; i >n; i++) {
			for (int j = i; j >n-1 ; j++) {
				System.out.print(" ");
			}
			for (int j =0 ; j>=0 ; j--) {
				System.out.print(n++);
			}
			System.out.println();
		}
		
		
	}

}
